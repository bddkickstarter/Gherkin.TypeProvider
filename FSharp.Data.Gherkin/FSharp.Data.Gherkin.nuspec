    <file src="..\FSharp.Data.Gherkin.DesignTime\bin\Release\netstandard2.0\FSharp.Data.Gherkin.DesignTime.dll" target="ref/netstandard2.0" /> -->

    <file src="..\FSharp.Data.Gherkin\bin\Release\netstandard2.0\FSharp.Data.Gherkin.dll" target="ref/net45" />
